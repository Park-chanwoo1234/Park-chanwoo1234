# 나선형 배열
x=-1
y=0
d=-1
nums=0
size=5
n=[[0]*5 for n in range(5)]
while True:
  d *=-1
  for _ in range(size):
    n +=1
    x+=d
    n[y][x]=nums
  size-=1
  if size<1: break;
  for _ in range(size):
    n +=1
    y+=d
    n[y][x]=nums
    
for i in range(5):
  for j in range(5):
    print(n[i][j], end=" ")
  print()
